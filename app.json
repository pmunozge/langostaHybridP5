{
  "expo": {
    "name": "ereselmejor",
    "slug": "ereselmejor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/iconNotification.png"
    },
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashLangosta.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
